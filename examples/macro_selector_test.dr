// Test that selectors work like macros (compile-time only)

fn test_valid_selector() {
    // This should work - selector in execution context
    as sel::AllPlayers { dist = ..10 } {
        say "Hello nearby players!"
    }
}

fn test_invalid_selector() {
    // This should ERROR - cannot store selector in variable
    let s = sel::AllPlayers { };
    as s {
        say "This should not compile"
    }
}
